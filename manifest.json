{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Overwolf Manifest Validator",
		"version":"0.1.0",
		"minimum-overwolf-version":"0.101.26",
		"author":"Developer Name",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Demo App"
	},

	"permissions": [
		"FileSystem"
	],
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":1024, "height":800},
					"min_size": {"width":500, "height":500}
				}
		},
		"externally_connectable": {
			  "matches": [
				"http://pastebin.com",
				"http://www.jsonschemavalidator.net"
			  ]
		}
	}
}